<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.eyhackathon">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.EYHackathon">
        <activity android:name=".Dashboard"></activity>
        <activity android:name=".Courses" />
        <activity android:name=".ReadChatsTest" />
        <activity android:name=".MessageActivity" />
        <activity android:name=".BuddyPairing" /> <!-- <activity android:name=".Dummy_Registration" /> -->
        <activity
            android:name=".Tasks"
            android:label="@string/title_activity_tasks" />
        <activity android:name=".Registration_Page" />
        <activity android:name=".Login" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>